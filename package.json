{
  "$schema": "https://enconvo.com/schemas/extension.json",
  "name": "reranker_providers",
  "version": "0.0.3",
  "description": "Reranker Providers",
  "title": "Reranker Providers",
  "icon": "icon.png",
  "author": "EnconvoAI",
  "license": "MIT",
  "minAppVersion": "1.8.8",
  "categories": [
    "Provider"
  ],
  "type": "module",
  "commands": [
    {
      "name": "enconvo_ai",
      "title": "EnConvo Cloud Plan",
      "description": "Reranker with Enconvo Cloud Plan , learn more : [docs](https://docs.enconvo.com/docs/providers/reranker)",
      "icon": "enconvo.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "modelName",
          "description": "The model to generate the completion.",
          "type": "dropdown",
          "required": true,
          "title": "Model Name",
          "default": "voyage/rerank-2",
          "data": [
            {
              "title": "Voyage Rerank-2 (40 points per invoke)",
              "value": "voyage/rerank-2",
              "context": 16000
            },
            {
              "title": "Voyage Rerank-2 Lite (10 points per invoke)",
              "value": "voyage/rerank-2-lite",
              "context": 8000
            }
          ]
        }
      ]
    },
    {
      "name": "voyage_ai",
      "title": "Voyage AI",
      "description": "Reranker with Voyage AI , learn more : [docs](https://docs.voyageai.com/docs/reranker)",
      "icon": "voyage.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "modelName",
          "description": "The model to generate the completion.",
          "type": "dropdown",
          "required": true,
          "title": "Model Name",
          "default": "rerank-2",
          "data": [
            {
              "title": "rerank-2",
              "value": "rerank-2",
              "context": 16000
            },
            {
              "title": "rerank-2-lite",
              "value": "rerank-2-lite",
              "context": 8000
            }
          ]
        },
        {
          "name": "apiKey",
          "description": "The API key to use for the Voyage AI API.",
          "type": "password",
          "default": "",
          "required": true,
          "title": "API Key"
        }
      ]
    },
    {
      "name": "siliconflow",
      "title": "SiliconFlow",
      "description": "Reranker with [SiliconFlow](https://siliconflow.cn)",
      "icon": "siliconflow.png",
      "mode": "provider",
      "preferences": [
        {
          "name": "modelName",
          "description": "The model to generate the reranker.",
          "type": "dropdown",
          "required": false,
          "title": "Model Name",
          "default": "netease-youdao/bce-reranker-base_v1",
          "data": [
            {
              "title": "BAAI/bge-reranker-v2-m3",
              "value": "BAAI/bge-reranker-v2-m3",
              "context": 8000
            },
            {
              "title": "netease-youdao/bce-reranker-base_v1",
              "value": "netease-youdao/bce-reranker-base_v1",
              "context": 512
            }
          ]
        },
        {
          "name": "openAIApiKey",
          "description": "How to get api key? [ðŸ”‘here](https://cloud.siliconflow.cn/account/ak)",
          "type": "password",
          "required": false,
          "title": "API Key",
          "default": "",
          "defaultProxy": "KEY_SILICONFLOW_APIKEY",
          "placeholder": "sk-********"
        },
        {
          "name": "baseUrl",
          "description": "OpenAI api base url",
          "type": "textfield",
          "required": false,
          "title": "OpenAI Api Base Url",
          "default": "https://api.siliconflow.cn/v1",
          "placeholder": "Api Base Url",
          "visibility": "hidden"
        }
      ]
    }
  ],
  "dependencies": {
    "@enconvo/api": "^0.1.152",
    "axios": "^1.6.8"
  },
  "devDependencies": {
    "@types/node": "^18.19.31",
    "eslint": "^8.57.0",
    "prettier": "^2.8.8",
    "tsup": "^7.2.0",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --list-different \"**/*.ts\"",
    "build": "enconvo",
    "dev": "enconvo --dev"
  }
}
